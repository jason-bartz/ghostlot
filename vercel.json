{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "typescript": {
          "ignoreBuildErrors": true
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/",
      "headers": { "x-matched-path": "/" },
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "github": {
    "silent": true
  }
}